    <!--disable_collisions link1="positioner_link_2" link2="link_2" reason="Never" />
    <disable_collisions link1="positioner_link_2" link2="link_3" reason="Never" />
    <disable_collisions link1="positioner_link_2" link2="link_4" reason="Never" />
    <disable_collisions link1="positioner_link_2" link2="link_5" reason="Never" />
    <disable_collisions link1="positioner_link_2" link2="link_6" reason="Never" /-->

</robot>
